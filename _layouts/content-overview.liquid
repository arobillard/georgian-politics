---
layout: default
---
<header class="page-header pab-t-2 bg-primary">
  <div class="wrapper gutter pad-t-2 pab-b">
    <h1 class="push-0">{{ page.title }}</h1>
    {% comment %}
    <ol class="breadcrumbs milli list-group-inline" class="no-print">
      {% capture url_parts %}{{ page.url | replace:'/'," " }}{% endcapture %}
      {% capture maybe_index_html %}{{ page.url | split: '/' | last }}{% endcapture %}
      {% if page.breadcrumb_limit %}
        {% assign num_parts = page.breadcrumb_limit %}
      {% else %}
        {% capture num_parts %}{{ url_parts | number_of_words | minus: 1 }}{% endcapture %}
      {% endif %}
      {% assign previous="" %}
      <li title="print working directory">
        <a href="/">Home</a>
      </li>
      {% for unused in (1..num_parts) %}
        {% capture first_word %}{{ url_parts | truncatewords:1 | remove:"..."}}{% endcapture %}
        {% capture previous %}{{ previous }}/{{ first_word }}{% endcapture %}
      <li>
        <a href="{{previous}}">{{ first_word | replace:'-', ' ' }}</a>
      </li>
        {% capture url_parts %}{{ url_parts |remove_first:first_word }}{% endcapture %}
      {% endfor %}
    </ol>
    {% endcomment %}
  </div>
</header>
<div class="wrapper">
  <ol class="breadcrumbs island push-0 list-group-inline" class="no-print">
    {% capture url_parts %}{{ page.url | replace:'/'," " }}{% endcapture %}
    {% capture maybe_index_html %}{{ page.url | split: '/' | last }}{% endcapture %}
    {% if page.breadcrumb_limit %}
      {% assign num_parts = page.breadcrumb_limit %}
    {% else %}
      {% capture num_parts %}{{ url_parts | number_of_words | minus: 1 }}{% endcapture %}
    {% endif %}
    {% assign previous="" %}
    <li title="print working directory">
      <a href="/">Home</a>
    </li>
    {% for unused in (1..num_parts) %}
      {% capture first_word %}{{ url_parts | truncatewords:1 | remove:"..."}}{% endcapture %}
      {% capture previous %}{{ previous }}/{{ first_word }}{% endcapture %}
    <li>
      <a href="{{previous}}">{{ first_word | replace:'-', ' ' }}</a>
    </li>
      {% capture url_parts %}{{ url_parts |remove_first:first_word }}{% endcapture %}
    {% endfor %}
  </ol>
</div>

{{ content }}
